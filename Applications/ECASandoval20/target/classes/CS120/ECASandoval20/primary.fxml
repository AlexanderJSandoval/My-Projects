<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="465.0" prefWidth="676.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CS120.ECASandoval20.PrimaryController">
   <children>
      <Label text="Hit or Pass" />
      <Button fx:id="primaryButton" onAction="#HitProc" text="Hit" />
      <Button mnemonicParsing="false" onAction="#passProc" text="Pass" />
      <Label text="Please close window after making your selection" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
